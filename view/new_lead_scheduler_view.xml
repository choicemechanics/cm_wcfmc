<?xml version="1.0"?>
<odoo>
    <data noupdate="1">
        <!-- Scheduler for Event Alarm-->
        <record forcecreate="True" id="ir_cron_scheduler_storage" model="ir.cron">
            <field name="name">Get New Leads</field>
            <field eval="True" name="active" />
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">1</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall" />
            <field eval="'scheduler.cm'" name="model" />
            <field eval="'get_new_leads_dummy'" name="function" />
        </record>
    </data>
</odoo>